@{
    ViewData["Title"] = "Schedule List";
}

<h2>Schedule List</h2>

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success">@TempData["SuccessMessage"]</div>
}

<table class="table">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Start Time</th>
            <th>End Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model)
        {
            <tr>
                <td>@schedule.SubjectName</td>
                <td>@schedule.StartTime.ToString("yyyy-MM-dd HH:mm")</td>
                <td>@schedule.EndTime.ToString("yyyy-MM-dd HH:mm")</td>
            </tr>
        }
    </tbody>
</table>
